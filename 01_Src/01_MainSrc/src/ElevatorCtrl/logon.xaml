        <Label Content="密码" Height="28" HorizontalAlignment="Left" Margin="113,90,0,0" Name="label2" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="168,90,0,0" Name="boxPassword" VerticalAlignment="Top" Width="120" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="168,61,0,0" Name="cmbUser" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
