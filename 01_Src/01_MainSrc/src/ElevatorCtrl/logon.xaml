        Title="登陆" Height="250" Width="450" Background="{x:Null}">
    <Grid>
        <Button Content="登陆" Height="23" HorizontalAlignment="Left" Margin="113,136,0,0" Name="btnLogon" VerticalAlignment="Top" Width="75" />
        <Button Content="退出" Height="23" HorizontalAlignment="Left" Margin="233,136,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" />
